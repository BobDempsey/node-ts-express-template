{
	"name": "node-ts-express-template",
	"version": "1.0.0",
	"description": "A Node.js TypeScript Express project template",
	"main": "dist/index.js",
	"scripts": {
		"dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
		"build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
		"start": "node dist/index.js",
		"clean": "rimraf dist",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:ci": "jest --ci --coverage --watchAll=false",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"typecheck": "tsc --noEmit",
		"prepare": "husky"
	},
	"keywords": [
		"nodejs",
		"node",
		"typescript",
		"express",
		"template",
		"http",
		"server",
		"environment",
		"env",
		"zod",
		"validation",
		"jest",
		"supertest",
		"testing",
		"lint",
		"format",
		"biome",
		"vscode",
		"configuration",
		"schema",
		"typesafe",
		"integration",
		"unit",
		"constants"
	],
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/BobDempsey/node-ts-template.git"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.6",
		"@commitlint/cli": "^20.2.0",
		"@commitlint/config-conventional": "^20.2.0",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.6",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^20.10.0",
		"@types/pino-pretty": "^4.7.5",
		"@types/supertest": "^6.0.3",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"husky": "^9.1.7",
		"jest": "^30.2.0",
		"lint-staged": "^16.2.7",
		"nodemon": "^3.0.2",
		"rimraf": "^5.0.5",
		"supertest": "^7.1.4",
		"ts-jest": "^29.4.5",
		"ts-node": "^10.9.1",
		"tsc-alias": "^1.8.16",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.3.3"
	},
	"engines": {
		"node": ">=24.0.0",
		"npm": ">=10.0.0"
	},
	"dependencies": {
		"@sentry/node": "^10.30.0",
		"cors": "^2.8.5",
		"dotenv": "^17.2.3",
		"express": "^5.2.1",
		"express-rate-limit": "^8.2.1",
		"helmet": "^8.1.0",
		"jsonwebtoken": "^9.0.3",
		"pino": "^10.1.0",
		"pino-pretty": "^13.1.3",
		"pino-sentry-transport": "^1.6.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"zod": "^4.1.12"
	},
	"lint-staged": {
		"*": "biome check --write --no-errors-on-unmatched"
	}
}
